        </maml:description>
        <command:parameterValue required="true" variableLength="false">Orchard.Environment.Configuration.ShellSettings</command:parameterValue>
        <dev:type>
          <maml:name>Orchard.Environment.Configuration.ShellSettings</maml:name>
          <maml:uri/>
        </dev:type>
