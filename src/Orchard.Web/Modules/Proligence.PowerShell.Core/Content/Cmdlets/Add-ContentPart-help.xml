    <!-- Input - Output section-->
    
    <command:inputTypes>
      
      <command:inputType>
        <dev:type>
          <maml:name>Orchard.Environment.Configuration.ShellSettings</maml:name>
          <maml:uri/>
          <maml:description />
        </dev:type>
        <maml:description>
          <maml:para>You can pipe instances of the ShellSettings class to Add-ContentPart. This object can be acquired by using the Get-ChildItem cmdlet on the Orchard PSDrive contents or by using the Get-Tenant cmdlet.</maml:para>
        </maml:description>
      </command:inputType>
      
      <command:inputType>
        <dev:type>
          <maml:name>Orchard.ContentManagement.MetaData.Models.ContentTypeDefinition</maml:name>
          <maml:uri/>
          <maml:description />
        </dev:type>
        <maml:description>
          <maml:para>You can pipe instances of the ContentTypeDefinition class to Add-ContentPart. This object can be acquired by using the Get-ChildItem cmdlet on the Orchard PSDrive contents or by using the Get-ContentType cmdlet.</maml:para>
        </maml:description>
      </command:inputType>

      <command:inputType>
        <dev:type>
          <maml:name>Orchard.ContentManagement.MetaData.Models.ContentPartDefinition</maml:name>
          <maml:uri/>
          <maml:description />
        </dev:type>
        <maml:description>
          <maml:para>You can pipe instances of the ContentPartDefinition class to Add-ContentPart. This object can be acquired by using the Get-ChildItem cmdlet on the Orchard PSDrive contents or by using the Get-ContentPartDefinition cmdlet.</maml:para>
        </maml:description>
      </command:inputType>
      
    </command:inputTypes>

    <command:returnValues />

    <!-- Error section-->
    
    <command:terminatingErrors />
    <command:nonTerminatingErrors />

    <!-- Notes section  -->

    <maml:alertSet>
      <maml:title></maml:title>
      <maml:alert>
        <maml:para>
          When no tenant is specified, then the command will use the tenant at the current location in the Orchard drive. If the location is not in any Orchard tenant, then the Default tenant is used.
        </maml:para>
        <maml:para>
          If the content type already has the specified content part, then the cmdlet has no effect.
        </maml:para>
        <maml:para />
      </maml:alert>
    </maml:alertSet>

    <!-- Example section  -->
    
    <command:examples>
      
      <command:example>
        <maml:title>
          -------------------------- EXAMPLE 1 --------------------------
        </maml:title>
        <maml:introduction>
          <maml:para>Orchard:\Tenants\MyTenant&gt; </maml:para>
        </maml:introduction>
        <dev:code>
          Add-ContentPart BlogPost TitlePart
        </dev:code>
        <dev:remarks>
          <maml:para>Description</maml:para>
          <maml:para>-----------</maml:para>
          <maml:para>This command adds the 'TitlePart' content part to the 'BlogPost' content type. In this case the tenant from the current location in the Orchard drive is used, since no tenant name or object was specified as a parameter.</maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
        </dev:remarks>
        <command:commandLines>
          <command:commandLine>
            <command:commandText></command:commandText>
          </command:commandLine>
        </command:commandLines>
      </command:example>

      <command:example>
        <maml:title>
          -------------------------- EXAMPLE 2 --------------------------
        </maml:title>
        <maml:introduction>
          <maml:para>Orchard:\&gt; </maml:para>
        </maml:introduction>
        <dev:code>
          Add-ContentPart BlogPost TitlePart -Tenant 'MyTenant'
        </dev:code>
        <dev:remarks>
          <maml:para>Description</maml:para>
          <maml:para>-----------</maml:para>
          <maml:para>This command adds the 'TitlePart' content part to the 'BlogPost' content type. In this case the tenant is specified explicitly using the 'Tenant' parameter.</maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
