    <!-- Cmdlet details section-->
    
    <command:details>
      <command:name>Add-ContentPart</command:name>
      <maml:description>
        <maml:para>Adds a content part to an Orchard content type.</maml:para>
      </maml:description>
      <maml:copyright>
        <maml:para></maml:para>
      </maml:copyright>
      <command:verb>Add</command:verb>
      <command:noun>ContentPart</command:noun>
      <dev:version></dev:version>
    </command:details>

    <!-- Cmdlet description section-->
    
    <maml:description>
      <maml:para>The Add-ContentPart cmdlet adds a content part to an Orchard content type.</maml:para>
    </maml:description>

    <!-- Cmdlet syntax section-->
    
    <command:syntax>

      <!-- ParameterSet: Default -->
      <command:syntaxItem>
        <maml:name>Add-ContentPart</maml:name>
        <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="false" position="1">
          <maml:name>ContentType</maml:name>
          <maml:description>
            <maml:para>Specifies the name of the content type to which the content part will be added.</maml:para>
          </maml:description>
          <command:parameterValue required="false" variableLength="true">string</command:parameterValue>
        </command:parameter>
        <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="false" position="2">
          <maml:name>ContentPart</maml:name>
          <maml:description>
            <maml:para>Specifies the name of the content part which will be added to the content type.</maml:para>
          </maml:description>
          <command:parameterValue required="false" variableLength="true">string</command:parameterValue>
        </command:parameter>
        <command:parameter required="false" variableLength="true" globbing="false" pipelineInput="false">
          <maml:name>Tenant</maml:name>
          <maml:description>
            <maml:para>Specifies the name of the Orchard tenant which contains the content type to which the content part will be added.</maml:para>
          </maml:description>
          <command:parameterValue required="false" variableLength="true">string</command:parameterValue>
        </command:parameter>
      </command:syntaxItem>

      <!-- ParameterSet: ContentTypeObject -->
      <command:syntaxItem>
        <maml:name>Add-ContentPart</maml:name>
        <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="true">
          <maml:name>ContentTypeObject</maml:name>
          <maml:description>
            <maml:para>Specifies the content type to which the content part will be added. This object can be acquired by using the Get-ChildItem cmdlet on the Orchard PSDrive contents or by using the Get-ContentType cmdlet. This parameter is required.</maml:para>
          </maml:description>
          <command:parameterValue required="false" variableLength="true">Orchard.ContentManagement.MetaData.Models.ContentTypeDefinition</command:parameterValue>
        </command:parameter>
        <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="false">
          <maml:name>ContentPart</maml:name>
          <maml:description>
            <maml:para>Specifies the name of the content part which will be added to the content type.</maml:para>
          </maml:description>
          <command:parameterValue required="false" variableLength="true">string</command:parameterValue>
        </command:parameter>
        <command:parameter required="false" variableLength="true" globbing="false" pipelineInput="false">
          <maml:name>Tenant</maml:name>
          <maml:description>
            <maml:para>Specifies the name of the Orchard tenant which contains the content type to which the content part will be added.</maml:para>
          </maml:description>
          <command:parameterValue required="false" variableLength="true">string</command:parameterValue>
        </command:parameter>
      </command:syntaxItem>

      <!-- ParameterSet: ContentPartObject -->
      <command:syntaxItem>
        <maml:name>Add-ContentPart</maml:name>
        <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="true">
          <maml:name>ContentPartObject</maml:name>
          <maml:description>
            <maml:para>Specifies the definition of the content part which will be added to the content type. This object can be acquired by using the Get-ChildItem cmdlet on the Orchard PSDrive contents or by using the Get-ContentPartDefinition cmdlet. This parameter is required.</maml:para>
          </maml:description>
          <command:parameterValue required="false" variableLength="true">Orchard.ContentManagement.MetaData.Models.ContentPartDefinition</command:parameterValue>
        </command:parameter>
        <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="false" position="1">
          <maml:name>ContentType</maml:name>
          <maml:description>
            <maml:para>Specifies the name of the content type to which the content part will be added.</maml:para>
          </maml:description>
          <command:parameterValue required="false" variableLength="true">string</command:parameterValue>
        </command:parameter>
        <command:parameter required="false" variableLength="true" globbing="false" pipelineInput="false">
          <maml:name>Tenant</maml:name>
          <maml:description>
            <maml:para>Specifies the name of the Orchard tenant which contains the content type to which the content part will be added.</maml:para>
          </maml:description>
          <command:parameterValue required="false" variableLength="true">string</command:parameterValue>
        </command:parameter>
      </command:syntaxItem>

      <!-- ParameterSet: TenantObject -->
      <command:syntaxItem>
        <maml:name>Add-ContentPart</maml:name>
        <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="false" position="1">
          <maml:name>ContentType</maml:name>
          <maml:description>
            <maml:para>Specifies the name of the content type to which the content part will be added.</maml:para>
          </maml:description>
          <command:parameterValue required="false" variableLength="true">string</command:parameterValue>
        </command:parameter>
        <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="false" position="2">
          <maml:name>ContentPart</maml:name>
          <maml:description>
            <maml:para>Specifies the name of the content part which will be added to the content type.</maml:para>
          </maml:description>
          <command:parameterValue required="false" variableLength="true">string</command:parameterValue>
        </command:parameter>
        <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="true">
          <maml:name>TenantObject</maml:name>
          <maml:description>
            <maml:para>Specifies the object that represents the Orchard tenant which contains the content type to which the content part will be added. This object can be acquired by using the Get-ChildItem cmdlet on the Orchard PSDrive contents or by using the Get-Tenant cmdlet. This parameter is required.</maml:para>
          </maml:description>
          <command:parameterValue required="false" variableLength="true">Orchard.Environment.Configuration.ShellSettings</command:parameterValue>
        </command:parameter>
      </command:syntaxItem>

      <!-- ParameterSet: AllTenants -->
      <command:syntaxItem>
        <maml:name>Add-ContentPart</maml:name>
        <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="false" position="1">
          <maml:name>ContentType</maml:name>
          <maml:description>
            <maml:para>Specifies the name of the content type to which the content part will be added.</maml:para>
          </maml:description>
          <command:parameterValue required="false" variableLength="true">string</command:parameterValue>
        </command:parameter>
        <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="false" position="2">
          <maml:name>ContentPart</maml:name>
          <maml:description>
            <maml:para>Specifies the name of the content part which will be added to the content type.</maml:para>
          </maml:description>
          <command:parameterValue required="false" variableLength="true">string</command:parameterValue>
        </command:parameter>
        <command:parameter required="true" variableLength="false" globbing="false" pipelineInput="false">
          <maml:name>AllTenants</maml:name>
          <maml:description>
            <maml:para>Specifies that the command should add the content part to the content type in all running Orchard tenants.</maml:para>
          </maml:description>
        </command:parameter>
      </command:syntaxItem>
      
    </command:syntax>

