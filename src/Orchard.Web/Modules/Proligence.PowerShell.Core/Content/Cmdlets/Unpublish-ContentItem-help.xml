    <!-- Cmdlet details section-->
    
    <command:details>
      <command:name>Unpublish-ContentItem</command:name>
      <maml:description>
        <maml:para>Unpublishes an Orchard content item.</maml:para>
      </maml:description>
      <maml:copyright>
        <maml:para></maml:para>
      </maml:copyright>
      <command:verb>Unpublish</command:verb>
      <command:noun>ContentItem</command:noun>
      <dev:version></dev:version>
    </command:details>

    <!-- Cmdlet description section-->
    
    <maml:description>
      <maml:para>The Unpublish-ContentItem cmdlet unpublishes an Orchard content item.</maml:para>
    </maml:description>

