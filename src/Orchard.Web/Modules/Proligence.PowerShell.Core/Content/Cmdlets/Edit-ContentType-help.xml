    <!-- Cmdlet details section-->
    
    <command:details>
      <command:name>Edit-ContentType</command:name>
      <maml:description>
        <maml:para>Modifies an existing Orchard content type.</maml:para>
      </maml:description>
      <maml:copyright>
        <maml:para></maml:para>
      </maml:copyright>
      <command:verb>Edit</command:verb>
      <command:noun>ContentType</command:noun>
      <dev:version></dev:version>
    </command:details>

    <!-- Cmdlet description section-->
    
    <maml:description>
      <maml:para>The Edit-ContentType cmdlet modifies an existing Orchard content type.</maml:para>
    </maml:description>

