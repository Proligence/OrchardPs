        </maml:introduction>
        <dev:code>
          New-ContentType MyType -Parts ("CommonPart", "TitlePart") -Settings @{"Stereotype" = "Widget"; "MySetting" = "MyValue"}
        </dev:code>
        <dev:remarks>
          <maml:para>Description</maml:para>
