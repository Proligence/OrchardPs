    <!-- Cmdlet details section-->
    
    <command:details>
      <command:name>New-ContentType</command:name>
      <maml:description>
        <maml:para>Creates a new Orchard content type.</maml:para>
      </maml:description>
      <maml:copyright>
        <maml:para></maml:para>
      </maml:copyright>
      <command:verb>New</command:verb>
      <command:noun>ContentType</command:noun>
      <dev:version></dev:version>
    </command:details>

    <!-- Cmdlet description section-->
    
    <maml:description>
      <maml:para>The New-ContentType cmdlet creates a new Orchard content type.</maml:para>
    </maml:description>

