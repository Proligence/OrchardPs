    <!-- Cmdlet details section-->
    
    <command:details>
      <command:name>Edit-ContentPartDefinition</command:name>
      <maml:description>
        <maml:para>Modifies the settings of an Orchard content part definition.</maml:para>
      </maml:description>
      <maml:copyright>
        <maml:para></maml:para>
      </maml:copyright>
      <command:verb>Edit</command:verb>
      <command:noun>ContentPartDefinition</command:noun>
      <dev:version></dev:version>
    </command:details>

    <!-- Cmdlet description section-->
    
    <maml:description>
      <maml:para>The Edit-ContentPartDefinition cmdlet modifies the settings of an Orchard content part definition.</maml:para>
    </maml:description>

    <!-- Cmdlet syntax section-->
    
    <command:syntax>

      <!-- ParameterSet: Default -->
      <command:syntaxItem>
        <maml:name>Edit-ContentPartDefinition</maml:name>
        <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="false" position="1">
          <maml:name>Name</maml:name>
          <maml:description>
            <maml:para>Specifies the name of the content part which definition will be modified.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="true">string</command:parameterValue>
        </command:parameter>
        <command:parameter required="false" variableLength="true" globbing="false" pipelineInput="false">
          <maml:name>Tenant</maml:name>
          <maml:description>
            <maml:para>Specifies the name of the Orchard tenant to which the modified content part belongs.</maml:para>
          </maml:description>
          <command:parameterValue required="false" variableLength="true">string</command:parameterValue>
        </command:parameter>
        <command:parameter required="false" variableLength="true" globbing="false" pipelineInput="false">
          <maml:name>Description</maml:name>
          <maml:description>
            <maml:para>Specifies the new description of the content part.</maml:para>
          </maml:description>
          <command:parameterValue required="false" variableLength="true">string</command:parameterValue>
        </command:parameter>
        <command:parameter required="false" variableLength="true" globbing="false" pipelineInput="false">
          <maml:name>Attachable</maml:name>
          <maml:description>
            <maml:para>If set to $true, the content part will be marked as attachable. If set to $false, the content part will be marked as not attachable.</maml:para>
          </maml:description>
          <command:parameterValue required="false" variableLength="true">bool</command:parameterValue>
        </command:parameter>
        <command:parameter required="false" variableLength="true" globbing="false" pipelineInput="false">
          <maml:name>Settings</maml:name>
          <maml:description>
            <maml:para>Specifies custom settings for the content part.</maml:para>
          </maml:description>
          <command:parameterValue required="false" variableLength="true">ArrayList</command:parameterValue>
        </command:parameter>
      </command:syntaxItem>

