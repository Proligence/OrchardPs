    <!-- Cmdlet details section-->
    
    <command:details>
      <command:name>New-Tenant</command:name>
      <maml:description>
        <maml:para>Creates a new Orchard tenant.</maml:para>
      </maml:description>
      <maml:copyright>
        <maml:para></maml:para>
      </maml:copyright>
      <command:verb>New</command:verb>
      <command:noun>Tenant</command:noun>
      <dev:version></dev:version>
    </command:details>

    <!-- Cmdlet description section-->
    
    <maml:description>
      <maml:para>The New-Tenant cmdlet creates a new Orchard tenant.</maml:para>
    </maml:description>

    <!-- Cmdlet syntax section-->
    
    <command:syntax>
      
      <command:syntaxItem>
        <maml:name>New-Tenant</maml:name>
        <command:parameter required="true" variableLength="false" globbing="false" pipelineInput="false" position="1">
          <maml:name>Name</maml:name>
          <maml:description>
            <maml:para>Specifies the name of the new tenant as it will be displayed in the admin.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">string</command:parameterValue>
        </command:parameter>
        <command:parameter required="true" variableLength="false" globbing="false" pipelineInput="false" position="2">
          <maml:name>RequestUrlHost</maml:name>
          <maml:description>
            <maml:para>Specifies the host name of the new tenant. Example: If host is "orchardproject.net", the tenant site URL is "http://orchardproject.net/". You may define multiple domains using the comma (,) as a separator. Use the '*.' prefix to map all subdomains.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">string</command:parameterValue>
        </command:parameter>
        <command:parameter required="false" variableLength="false" globbing="false" pipelineInput="false">
          <maml:name>RequestUrlPrefix</maml:name>
          <maml:description>
            <maml:para>Specifies the URL prefix of the new tenant. Example: If prefix is "site1", the tenant URL prefix is "http://orchardproject.net/site1".</maml:para>
          </maml:description>
          <command:parameterValue required="false" variableLength="false">string</command:parameterValue>
        </command:parameter>
        <command:parameter required="false" variableLength="false" globbing="false" pipelineInput="false">
          <maml:name>DataConnectionString</maml:name>
          <maml:description>
            <maml:para>Specifies the connection string to the database of the new tenant.</maml:para>
          </maml:description>
          <command:parameterValue required="false" variableLength="false">string</command:parameterValue>
        </command:parameter>
        <command:parameter required="false" variableLength="false" globbing="false" pipelineInput="false">
          <maml:name>DataProvider</maml:name>
          <maml:description>
            <maml:para>Specifies the name of the data provider of the new tenant.</maml:para>
          </maml:description>
          <command:parameterValue required="false" variableLength="false">string</command:parameterValue>
        </command:parameter>
        <command:parameter required="false" variableLength="false" globbing="false" pipelineInput="false">
          <maml:name>DataTablePrefix</maml:name>
          <maml:description>
            <maml:para>Specifies the data table prefix of the new tenant.</maml:para>
          </maml:description>
          <command:parameterValue required="false" variableLength="false">string</command:parameterValue>
        </command:parameter>
      </command:syntaxItem>
      
      <command:syntaxItem>
        <maml:name>New-Tenant</maml:name>
        <command:parameter required="true" variableLength="false" globbing="false" pipelineInput="false" position="1">
          <maml:name>Tenant</maml:name>
          <maml:description>
            <maml:para>Specifies the object which represents the Orchard tenant to create.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">ShellSettings</command:parameterValue>
        </command:parameter>
      </command:syntaxItem>
      
    </command:syntax>

